{
  "productName": "Redis Studio",
  "version": "0.0.0",
  "identifier": "org.nigel.redisstudio",
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true,
    "windows": [
      {
        "fullscreen": false,
        "height": 720,
        "hiddenTitle": true,
        "resizable": true,
        "title": "Byte Studio",
        "transparent": false,
        "visible": false,
        "titleBarStyle": "Overlay",
        "width": 1200,
        "theme": "Dark",
        "label": "main"
      },
      {
        "height": 156,
        "label": "datasource-dropdown",
        "url": "windows/datasource-dropdown.html",
        "width": 400,
        "fullscreen": false,
        "hiddenTitle": true,
        "resizable": false,
        "title": "DataSourceDropdown",
        "visible": false,
        "decorations": false,
        "transparent": true,
        "titleBarStyle": "Overlay",
        "theme": "Dark"
      },
      {
        "label": "datasource-database-selector",
        "url": "windows/datasource-database-selector.html",
        "fullscreen": false,
        "hiddenTitle": true,
        "resizable": false,
        "title": "DataSourceDBSelector",
        "visible": false,
        "decorations": false,
        "transparent": true,
        "titleBarStyle": "Overlay",
        "theme": "Dark"
      },
      {
        "label": "spotlight-search",
        "shadow": false,
        "fullscreen": false,
        "hiddenTitle": true,
        "maximizable": false,
        "width": 502,
        "height": 26,
        "resizable": false,
        "alwaysOnTop": true,
        "transparent": true,
        "closable": false,
        "visible": false,
        "titleBarStyle": "Overlay",
        "decorations": false,
        "visibleOnAllWorkspaces": true,
        "url": "windows/spotlight-search.html"
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": ["../windows/*"]
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
